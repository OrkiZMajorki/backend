type Band {
    id: ID !
    name: String !
    description: String
    songUrl: String
    songName: String
    imageUrl: String
    citiesIds: [ID]
    cities: [String]
    musicGenresIds: [ID]
    musicGenres: [String]
}

extend type Query {
    findBandsByGenreAndCity(genres: [ID] !, cities: [ID] !): [Band]
}

input BandDTO {
    name: String !
    description: String
    songUrl: String
    songName: String
    imageUrl: String
    citiesIds: [ID]
    musicGenresIds: [ID]
}