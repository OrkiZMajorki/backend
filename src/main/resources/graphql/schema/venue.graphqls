type Venue {
    id: ID !
    name: String !
    capacity: Int
    cityId: ID
    city: String
    musicGenres: [String]
    user: User
}

extend type Mutation {
    createVenue(venue: VenueDTO !): Venue
}

input VenueDTO {
    name: String !
    capacity: Int
    cityId: ID
    musicGenres: [String]
    user: UserDTO
}