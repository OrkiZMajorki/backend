type Venue {
    id: ID !
    name: String !
    capacity: Int
    cityId: ID
    city: String
    musicGenres: [String]
}

extend type Mutation {
    createVenue(venue: VenueDTO !, userId: ID !): Venue
}

input VenueDTO {
    name: String !
    capacity: Int
    cityId: ID
    musicGenres: [String]
}